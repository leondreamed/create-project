{
	"name": "@leonzalion/create-ts-project",
	"version": "1.0.5",
	"main": "dist/index.js",
	"license": "MIT",
	"type": "module",
	"bin": {
		"create-ts-project": "dist/index.js"
	},
	"prettier": "@leonzalion/prettier-config",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"scripts": {
		"start": "node --loader ts-node/esm ./src/index.ts",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest",
		"build": "rimraf dist && tsc && tsc-alias -p tsconfig.json",
		"prepublish": "pnpm build",
		"lint": "eslint --fix src",
		"prettier": "prettier --write src",
		"tc": "tsc --noEmit"
	},
	"dependencies": {
		"commander": "^8.3.0",
		"execa": "^6.0.0",
		"replace-in-file": "^6.3.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^15.0.0",
		"@commitlint/config-conventional": "^15.0.0",
		"@leonzalion/eslint-config": "latest",
		"@leonzalion/markdownlint-config": "latest",
		"@leonzalion/prettier-config": "latest",
		"@leonzalion/tsconfig": "latest",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"eslint": "^8.4.1",
		"execa": "^6.0.0",
		"husky": "^7.0.4",
		"jest": "^27.4.5",
		"lint-staged": "^12.1.2",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.1",
		"ts-node": "^10.4.0",
		"tsc-alias": "^1.4.2",
		"typescript": "^4.5.2"
	}
}
